	targetNamespace="http://roskazna.ru/gisgmp/xsd/116/PackageStatusRequest"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:complexType name="PackageStatusRequestType">
		<xsd:annotation>
			<xsd:documentation>Запрос статуса обработки пакета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PackageID" type="xsd:ID">
				<xsd:annotation>
					<xsd:documentation>идентификатор пакета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
